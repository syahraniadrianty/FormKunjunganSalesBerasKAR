{
    "name": "nb_google/apiclient",
    "type": "library",
    "description": "Client library for NetBase Team Developer Customize from API Google",
    "keywords": ["google"],
    "homepage": "https://github.com/mrchienbmq/googleapiclient",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.4",
        "nb_google/auth": "v1.10",
        "nb_google/apiclient-services": "^0.139",
        "phpseclib/phpseclib": "~0.3.10||~2.0",
        "guzzlehttp/guzzle": "~5.3.1||~6.0||~7.0",
        "guzzlehttp/psr7": "^1.2",
        "monolog/monolog": "^1.17|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/dom-crawler": "~2.1",
        "symfony/css-selector": "~2.1",
        "cache/filesystem-adapter": "^0.3.2",
        "phpcompatibility/php-compatibility": "^9.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "composer/composer": "^1.10"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": {
            "Google_": "src/"
        },
        "classmap": [
            "src/Google/Service/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
