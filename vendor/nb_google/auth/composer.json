{
  "name": "nb_google/auth",
  "type": "library",
  "description": "Client library for NetBase Team Developer Customize from API Google",
  "keywords": ["google", "oauth2", "authentication"],
  "homepage": "https://github.com/mrchienbmq/auth",
  "license": "Apache-2.0",
  "support": {
    "docs": "https://googleapis.github.io/google-auth-library-php/master/"
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "^5.3.1|^6.2.1|^7.0",
    "guzzlehttp/psr7": "^1.2",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "guzzlehttp/promises": "0.1.1|^1.3",
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^4.8.36|^5.7",
    "sebastian/comparator": ">=1.2.3",
    "phpseclib/phpseclib": "^2",
    "kelvinmo/simplejwt": "^0.2.5"
  },
  "suggest": {},
  "autoload": {
    "psr-4": {
      "Google\\Auth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Google\\Auth\\Tests\\": "tests"
    }
  }
}
